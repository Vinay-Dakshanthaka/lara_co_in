                          <span class="fa fa-star rating-chk"></span>
                          <span class="fa fa-star rating-chk"></span>
                          <span class="fa fa-star rating-chk"></span>
                          <span class="fa fa-star rating-chk"></span>
                          <span class="fa fa-star rating-unchk"></span>
                        </div>
                        <div class="col-sm-12">
                          <p class="m-0 pt-3">{{tt.testimonialDesc}}</p><hr>
                        </div>                        
                    </div>                    
                  </div>
                </div>
              </div>                            
            </div>
          </div>
        </div>
      </div>

      <!-- Modal footer -->
      <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
      </div>

    </div>
  </div>
</div>
